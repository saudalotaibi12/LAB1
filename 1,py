#This is the code I did it of my own
x = 6

if x > 10:
    print("x is greater than 10")
    print("This is inside the first suite")
elif x > 5 and x <= 10:
    print("x is greater than 5 but not greater than 10")
    print("This is inside the second suite")
else:
    print("x is 5 or less")
    print("This is inside the third suite")
    
counter = 100 # An integer assignment
miles = 1000.0 # A floating point
name = "John" # A string
print (counter)
print (miles)
print (name) 
# Assign the same value to multiple variables
a = b = c = 1
print(a, b, c)   # Output: 1 1 1

# Assign different values to multiple variables
a, b, c = 1, 2, "john"
print(a, b, c)   # Output: 1 2 john
str = 'Hello World!'

print(str)        # Prints complete string
print(str[0])     # Prints first character of the string
print(str[2:5])   # Prints characters from index 2 to 4 (3rd to 5th character)
print(str[2:])    # Prints string starting from 3rd character
print(str * 2)    # Prints string two times
print(str + "TEST") # Prints concatenated string
list1 = ['abcd', 786, 2.23, 'john', 70.2]
tinylist = [123, 'john']

print(list1)        # Prints complete list
print(list1[0])     # Prints first element of the list
print(list1[1:3])   # Prints elements from index 1 to 2 (2nd to 3rd element)
print(list1[2:])    # Prints elements starting from index 2 (3rd element)
print(tinylist * 2) # Prints list two times
print(list1 + tinylist) # Prints concatenated lists

tuple1 = ('abcd', 786, 2.23, 'john', 70.2)
tinytuple = (123, 'john')

print(tuple1)         # Prints complete tuple
print(tuple1[0])      # Prints first element of the tuple
print(tuple1[1:3])    # Prints elements from index 1 to 2 (2nd to 3rd element)
print(tuple1[2:])     # Prints elements starting from i
# Creating dictionaries
dict1 = {}
dict1['one'] = "This is one"
dict1[2] = "This is two"

tinydict = {'name': 'john', 'code': 6734, 'dept': 'sales'}

# Accessing elements
print(dict1['one'])        # Prints value for 'one' key
print(dict1[2])            # Prints value for 2 key







